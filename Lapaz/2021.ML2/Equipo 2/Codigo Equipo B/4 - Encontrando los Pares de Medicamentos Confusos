as.csv')"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"MBZ_p8mlBO67","executionInfo":{"status":"ok","timestamp":1612391470881,"user_tz":360,"elapsed":442,"user":{"displayName":"EquipoB Saturdays","photoUrl":"","userId":"11393177357028755865"}},"outputId":"5e01705b-a2cb-4a20-9292-392c3d477c32"},"source":["print(pares_freq.head(100))"],"execution_count":null,"outputs":[{"output_type":"stream","text":["                    Par  Freq\n","0   FLIXONASE_FLAGENASE    12\n","1       ANALGEN_ANALFIN    12\n","2   FLAGENASE_FLIXONASE    12\n","3       TOBRADEX_TOBREX    12\n","4       TOBREX_TOBRADEX    12\n","..                  ...   ...\n","95    ARTROTEC_ARTRENAC     9\n","96    PREMARIN_PRIMALAN     9\n","97    ARTRENAC_ARTROTEC     9\n","98       CLIANE_CLIMENE     9\n","99    PRIMALAN_PREMARIN     9\n","\n","[100 rows x 2 columns]\n"],"name":"stdout"}]}]}